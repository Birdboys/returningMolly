    "IS_END": false
  },
  "day1Night_7": {
    "TEXT_TYPE": "think",
    "TEXT_SPEAKER": "dad",
    "TEXT_TEXT": "The trek to my current point was tiring but doable.",
    "NEXT_TEXT_ID": [
      "day1Night_8"
    ],
    "HAS_CHOICE": false,
    "IS_END": false
  },
  "day1Night_8": {
    "TEXT_TYPE": "think",
    "TEXT_SPEAKER": "dad",
