      "day1Night_5"
    ],
    "HAS_CHOICE": false,
    "IS_END": false
  },
  "day1Night_4c": {
    "TEXT_TYPE": "choice",
    "TEXT_SPEAKER": "dad",
    "TEXT_TEXT": "Molly?",
